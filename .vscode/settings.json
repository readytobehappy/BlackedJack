{
    "editor.tabSize": 2,
    "editor.formatOnSave": false,
    "files.eol": "\n",
    "vetur.format.defaultFormatter.html": "js-beautify-html",
    "[vue]": {
      "editor.defaultFormatter": "octref.vetur"
    },
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
    "eslint.validate": [
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact"
    ],
    "stylelint.validate": [
      "css",
      "less",
      "postcss",
      "vue"
    ],
    "makefile.configurations": [
    ]
  }
  